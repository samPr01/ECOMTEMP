{
  "name": "ss-stores",
  "version": "1.0.0",
  "description": "SS Stores - Full-featured e-commerce website",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && nodemon index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "ecommerce",
    "react",
    "nodejs",
    "express"
  ],
  "author": "SS Stores",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "jsonwebtoken": "^9.0.2"
  }
}
